ms/collide.ts","name":null,"original":{"line":49,"column":4},"generated":{"line":81,"column":0}},{"source":"../src/systems/collide.ts","name":null,"original":{"line":49,"column":4},"generated":{"line":81,"column":6}},{"source":"../src/systems/collide.ts","name":null,"original":{"line":49,"column":6},"generated":{"line":81,"column":8}},{"source":"../src/systems/collide.ts","name":null,"original":{"line":49,"column":7},"generated":{"line":81,"column":9}},{"source":"../src/systems/collide.ts","name":null,"original":{"line":49,"column":4},"generated":{"line":81,"column":22}},{"source":"../src/systems/collide.ts","name":null,"original":{"line":49,"column":21},"generated":{"line":81,"column":23}},{"source":"../src/systems/collide.ts","name":null,"original":{"line":49,"column":4},"generated":{"line":81,"column":42}},{"source":"../src/systems/collide.ts","name":null,"original":{"line":49,"column":42},"generated":{"line":81,"column":44}},{"source":"../src/systems/collide.ts","name":null,"original":{"line":49,"column":4},"generated":{"line":81,"column":57}},{"source":"../src/systems/collide.ts","name":null,"original":{"line":50,"column":3},"generated":{"line":82,"column":0}},{"source":"../src/systems/collide.ts","name":null,"original":{"line":6,"column":0},"generated":{"line":88,"column":0}},{"source":"../src/systems/collide.ts","name":null,"original":{"line":6,"column":0},"generated":{"line":88,"column":7}},{"source":"../src/systems/collide.ts","name":null,"original":{"line":6,"column":0},"generated":{"line":88,"column":8}},{"source":"../src/systems/collide.ts","name":null,"original":{"line":6,"column":0},"generated":{"line":88,"column":15}},{"source":"../src/systems/collide.ts","name":null,"original":{"line":6,"column":0},"generated":{"line":88,"column":18}},{"source":"../src/systems/collide.ts","name":null,"original":{"line":6,"column":0},"generated":{"line":88,"column":25}}],"sources":{"../src/systems/collide.ts":"import { EntityManager, System } from '../ecs_engine';\r\nimport { UpdateContext } from '../update_context';\r\nimport { Collisions } from '../components/collision';\r\nimport { HasToBeDeleted } from '../components/has_to_be_deleted';\r\n\r\nexport class Collide implements System<UpdateContext> {\r\n  name = 'Collide';\r\n\r\n  onUpdate(em: EntityManager<UpdateContext>, context: UpdateContext): void {\r\n    const collisionEntity = em.selectGlobal('collisions');\r\n    const collisions = collisionEntity.get('collisions') as Collisions;\r\n\r\n    const previousCollisionEntity = em.selectGlobal('previousCollision');\r\n    const prevCollision = previousCollisionEntity.get('previousCollision') as Collisions;\r\n\r\n    // solve collisions\r\n    for (const collision of collisions.collisions)\r\n    {\r\n      const componentsMapA = em.getEntityComponents(collision.idA);\r\n      const componentsMapB = em.getEntityComponents(collision.idB);\r\n\r\n      let ship = componentsMapA.get('Ship');\r\n      if(ship)\r\n      {\r\n        let idBeacon = '';\r\n        if (componentsMapA.get('Beacon')) {\r\n          idBeacon = collision.idA;\r\n        }\r\n        else if (componentsMapB.get('Beacon')) {\r\n          ship = componentsMapB.get('Ship');\r\n          idBeacon = collision.idB;\r\n        }\r\n\r\n        if (idBeacon) {\r\n          debugger;\r\n          const HasToBeDeletedComponent = new HasToBeDeleted();\r\n          em.addComponents(idBeacon, HasToBeDeletedComponent);\r\n        }\r\n      }\r\n\r\n      prevCollision.collisions.push(collision);\r\n    }\r\n\r\n    // Empty the new collisions\r\n    collisions.collisions = [];\r\n    em.addComponents('collisions', collisions);\r\n\r\n    // Archive the solved collisions\r\n    em.addComponents('previousCollision', prevCollision);\r\n  }\r\n}"},"lineCount":null}},"error":null,"hash":"07bf880bbdd8435ad0cee73e87c347a9","cacheData":{"env":{}}}